 .logo {
color: aliceblue;
font-family: 'Raleway', sans-serif;
font-size: 1.8em;
font-weight: 400;
text-transform: uppercase;
text-decoration: none;
letter-spacing: 2px;

}
 .navigation{
  position: relative;
  line-height: 75px; 
  transition-property: line-height;
}
header.sticky .navigation{
    line-height: 60px;
}
 .navigation .menu{
    position: relative;
    display: flex;
    justify-content: center;
    list-style: none;
    user-select:none;
}
.menu_item > a{
   color: aliceblue;
   font-size: 1em;
   text-decoration: none;
   margin: 20px;
   padding: 25px 0;
}
.menu_item>a:hover{
  color: black;
  transition: 0.3s;
}
.menu_item .sub-menu{
    position: absolute;
    background: rgb(100, 20, 20);
    top:74px;
   line-height: 40px;
   list-style: none;
   border-radius:  0 0 8px 8px;
   box-shadow: 0 5px 25px rgba(192, 26, 26, 0.2);
   pointer-events: none;
   transform: translateY(20px);
   opacity: 0;
   transition: 0.3s;
   transition-property: transform,opacity;
}
header.sticky .menu_item .sub-menu{
    top: 60px;
}
.menu_item:hover .sub-menu{
    pointer-events: all;
    transform: translateY(0);
    opacity: 1; 
    
}

.menu_item .sub-menu .sub-item{
    position: relative;
    padding: 7px 0;
    cursor: pointer;
    box-shadow: inset 0px -30px 5px -30px rgba(26, 12, 12, 0.251);

}

.menu_item .sub-menu .sub-item a{
    color: #fff;
    font-size: 1em;
    text-decoration: none;
    padding: 15px 30px;
  
}
.menu_item .sub-menu .sub-item:hover{
    background: black;
    
}
.menu_item .sub-menu .sub-item:last-child:hover{
    border-radius:0 0 8px 8px;
}

.menu-btn{
    display: none;
